package com.neotech.lesson08;

public class IntroToWhileLoop {
	
	public static void main(String[] args) {
		
		int time=8;
		
		while (time<12)
		
		{
			System.out.println(time);
			time++;//----->means time+1
		}
		
		//How to print number [20-20[?
		int a=20;
		
		while(a<30)//a<=29
			
		{
			System.out.println("Condition: "+(a<30));
			System.out.println(a);
			a++;
		}
	}

}
