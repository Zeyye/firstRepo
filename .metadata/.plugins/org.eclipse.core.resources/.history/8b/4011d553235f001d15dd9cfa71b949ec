package com.neotech.lesson23;

public class EmployyeTest {
	public static void main(String[] args) {

		Employee em1 = new Employee();
		em1.salary = 5600;
		em1.getPaid();
		System.out.println("===================");

		Constructor ct = new Constructor();

		ct.salary = 56000;
		ct.getPaid();
		ct.hourlyRate = 23;

	}
}
