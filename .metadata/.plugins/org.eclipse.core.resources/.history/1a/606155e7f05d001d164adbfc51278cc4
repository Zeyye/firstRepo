package com.neotech.review06;

public class Person {

	// instance variable
	public String name;
	int age;
	protected int weight;

	public Person() {

	}
//this is constructor
	public Person(String pName, int pAge) {
		name = pName;
		age = pAge;
	}
//this is method
	public void displayInfo() {
		System.out.println("Name: " + name + " age: " + age + " weight: " + weight);
	}

}
