package DenemeReview;

public class SwitchWithMissingBreaks {
	
	public static void main(String[] args) {
		
		
		int number=4;
		
		switch(number) 
		{
		case 2:
		System.out.println("2");	
		case 3:
			System.out.println("3");	
		case 4: 
			System.out.println("4");
		case 5:
		System.out.println("5");
		case 6:
		System.out.println("6");
		
		}
		
		
	}

}
