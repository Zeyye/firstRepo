package com.neotech.lesson06;

public class Homework3 {

	public static void main(String[] args) {
		
		
		// Write a Java Program to check whether number is Even or Odd.
		
		int number=7;
		//int number2=8;
		
		int remainder =number %2;
		
		if (remainder==0) 
		{
			System.out.println(number +"is even");
			
		}
		
		
		

	}

}
